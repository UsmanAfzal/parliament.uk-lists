- page_title = title("#{t('.procedure').capitalize.pluralize}")

.section--primary#content{ tabindex: "0" }
  .container
    %h1= page_title

    %ul.list--block
      - @procedures.each do |procedure|
        %li
          .card__details
            %h2= link_to(procedure.name, procedure_path(procedure.graph_id))
            %p= "#{t('.view_all')} #{procedure.name.downcase} #{t('.procedure')}."
